  <distributionManagement>
    <repository>
      <id>github</id>
      <url>https://maven.pkg.github.com/umjammer/FreeTTS</url>
    </repository>
  </distributionManagement>

  <build>
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>3.10.1</version>
        <configuration>
          <source>1.8</source>
          <target>1.8</target>
          <encoding>UTF-8</encoding>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-surefire-plugin</artifactId>
        <version>3.0.0-M7</version>
        <configuration>
          <includes>
            <include>**/*</include>
          </includes>
          <argLine>
            -Djava.util.logging.config.file=${project.build.testOutputDirectory}/logging.properties
          </argLine>
          <trimStackTrace>false</trimStackTrace>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-jar-plugin</artifactId>
        <version>3.3.0</version>
        <configuration>
          <archive>
            <manifestEntries>
              <Built-By>${user.name}</Built-By>
              <Main-Class>com.sun.speech.freetts.FreeTTS</Main-Class>
              <Implementation-Title>FreeTTS core library</Implementation-Title>
            </manifestEntries>
          </archive>
          <includes>
            <include>com/sun/speech/freetts/**</include>
            <include>de/dfki/lt/freetts/**</include>
          </includes>
          <excludes>
            <exclude>com/sun/speech/freetts/en/**</exclude>
            <exclude>com/sun/speech/freetts/jsapi/**</exclude>
            <exclude>de/dfki/lt/freetts/en/**</exclude>
            <exclude>de/dfki/lt/freetts/de/**</exclude>
            <exclude>de/dfki/lt/mbrola/**</exclude>
            <exclude>**/*.bin</exclude>
            <exclude>**/*.idx</exclude>
          </excludes>
        </configuration>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-antrun-plugin</artifactId>
        <version>3.1.0</version>
        <executions>
          <execution>
            <id>test-resource</id>
            <phase>generate-test-resources</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <copy file="src/main/voices/com/sun/speech/freetts/en/us/cmu_us_kal/cmu_us_kal.txt"
                      todir="${project.build.testOutputDirectory}/com/sun/speech/freetts/en/us/cmu_us_kal/" />
              </target>
            </configuration>
          </execution>

          <execution>
            <id>freetts_jsapi10_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/freetts_jsapi10-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/engine/**" />
                    <include name="com/sun/speech/freetts/jsapi/**" />
                  </fileset>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <section name="common">
                      <attribute name="Implementation-Title"
                                 value="FreeTTS JSAPI 1.0 layer" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>mbrola_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/mbrola-${project.version}.jar"
                     manifest="src/main/voices/de/dfki/lt/freetts/en/us/Manifest">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="de/dfki/lt/freetts/mbrola/**" />
                    <include name="de/dfki/lt/freetts/en/us/**" />
                  </fileset>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>en_us_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <property name="en_us_jar" value="${project.build.directory}/en_us-${project.version}.jar" />
                <manifestclasspath property="en_us_classpath" jarfile="${en_us_jar}">
                  <classpath>
                    <fileset file="${project.build.directory}/${project.artifactId}-${project.version}.jar" />
                  </classpath>
                </manifestclasspath>
                <jar jarfile="${en_us_jar}">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/*.class" />
                    <include name="com/sun/speech/freetts/en/us/*.class" />
                    <include name="com/sun/speech/freetts/en/us/*.txt" />
                  </fileset>
                  <fileset dir="src/main/voices">
                    <include name="**/cmu*lex*.txt" />
                  </fileset>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>cmulex_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/cmulex-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/us/cmulex_*.bin" />
                  </fileset>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <section name="common">
                      <attribute name="Implementation-Title" value="CMU Lexicon for en_us for FreeTTS" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>cmudict04_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/cmudict04-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/us/cmudict04_*.bin" />
                  </fileset>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <section name="common">
                      <attribute name="Implementation-Title" value="CMU Dictionary for en_us for FreeTTS" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>cmutimelex_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/cmutimelex-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/us/cmutimelex_*.bin" />
                  </fileset>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <section name="common">
                      <attribute name="Implementation-Title" value="CMU lexicon for en_us for FreeTTS" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>germanlex_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/germanlex-${project.version}.jar"
                     manifest="src/main/voices/de/dfki/lt/freetts/de/germanlex.Manifest">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="de/dfki/lt/freetts/de/germanlex_*.bin" />
                  </fileset>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>cmu_us_kal_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/cmu_us_kal-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/us/cmu_us_kal/*" />
                  </fileset>
                  <mappedresources>
                    <fileset dir="src/main/voices"
                             includes="com/sun/speech/freetts/en/us/cmu_us_kal/*.services" />
                    <globmapper from="*.services" to="META-INF/services/com.sun.speech.freetts.VoiceDirectory" />
                  </mappedresources>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <attribute name="Main-Class"
                               value="com.sun.speech.freetts.en.us.cmu_us_kal.KevinVoiceDirectory" />
                    <attribute name="FreeTTSVoiceDefinition" value="true" />
                    <section name="common">
                      <attribute name="Implementation-Title"
                                 value="default diphone voice for FreeTTS" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
          <execution>
            <id>cmu_time_awb_jar</id>
            <phase>package</phase>
            <goals>
              <goal>run</goal>
            </goals>
            <configuration>
              <target>
                <jar jarfile="${project.build.directory}/cmu_time_awb-${project.version}.jar">
                  <fileset dir="${project.build.outputDirectory}">
                    <include name="com/sun/speech/freetts/en/us/cmu_time_awb/*" />
                  </fileset>
                  <mappedresources>
                    <fileset dir="src/main/voices"
                             includes="com/sun/speech/freetts/en/us/cmu_time_awb/*.services" />
                    <globmapper from="*.services" to="META-INF/services/com.sun.speech.freetts.VoiceDirectory" />
                  </mappedresources>
                  <manifest>
                    <attribute name="Built-By" value="${user.name}" />
                    <attribute name="Main-Class"
                               value="com.sun.speech.freetts.en.us.cmu_time_awb.AlanVoiceDirectory" />
                    <attribute name="FreeTTSVoiceDefinition" value="true" />
                    <section name="common">
                      <attribute name="Implementation-Title"
                                 value="default diphone voice for FreeTTS" />
                    </section>
                  </manifest>
                </jar>
              </target>
            </configuration>
          </execution>
        </executions>
      </plugin>

      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-install-plugin</artifactId>
        <version>3.0.1</version>
