                <configuration>
                  <target>
                    <property name="project.resources.directory" value="src/main/voices" />
                    <property name="project.testResources.directory" value="${project.build.outputDirectory}" />
                    <java classname="com.sun.speech.freetts.lexicon.LetterToSoundImpl" fork="true">
                      <classpath>
