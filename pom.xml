            <executions>
              <execution>
                <id>kal8_db</id>
                <phase>compile</phase>
                <goals>
                  <goal>run</goal>
                </goals>
                <configuration>
                  <target>
                    <property name="project.resources.directory" value="src/main/voices" />
                    <property name="project.testResources.directory" value="${project.build.outputDirectory}" />
                    <mkdir dir="${project.testResources.directory}/com/sun/speech/freetts/en/us/cmu_us_kal" />
                    <java classname="com.sun.speech.freetts.diphone.DiphoneUnitDatabase" fork="true">
                      <classpath>
                        <path refid="maven.compile.classpath"/>
                        <path refid="maven.test.classpath"/>
                      </classpath>
                      <arg line="-src ${project.resources.directory}/com/sun/speech/freetts/en/us/cmu_us_kal" />
                      <arg line="-dest ${project.testResources.directory}/com/sun/speech/freetts/en/us/cmu_us_kal" />
                      <arg line="-generate_binary cmu_us_kal.txt" />
