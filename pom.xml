              <execution>
                <id>clunit_voice_db</id>
                <phase>compile</phase>
                <goals>
                  <goal>run</goal>
                </goals>
                <configuration>
                  <target if="${clunit_voice}">
                    <property name="project.resources.directory" value="src/main/voices" />
                    <property name="project.testResources.directory" value="${project.build.outputDirectory}" />
                    <property name="user_voice_base_path" value="${user_voice_base_path}" />
                    <property name="clunit_voice" value="${clunit_voice}" />
                    <mkdir dir="${project.testResources.directory}/${user_voice_base_path}/${clunit_voice}" />
                    <java classname="com.sun.speech.freetts.clunits.ClusterUnitDatabase" fork="true">
                      <classpath>
                        <path refid="maven.compile.classpath"/>
                        <path refid="maven.test.classpath"/>
                      </classpath>
                      <arg line="-src ${project.resources.directory}/${user_voice_base_path}" />
                      <arg line="-dest ${project.testResources.directory}/${user_voice_base_path}/${clunit_voice}" />
                      <arg line="-generate_binary ${clunit_voice}.txt" />
                    </java>
                  </target>
                </configuration>
              </execution>
              <execution>
                <id>diphone_voice_db</id>
                <phase>compile</phase>
                <goals>
