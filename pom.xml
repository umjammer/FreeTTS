                    <property name="diphone_voice" value="${diphone_voice}" />
                    <mkdir dir="${project.testResources.directory}/${user_voice_base_path}/${diphone_voice}" />
                    <java classname="com.sun.speech.freetts.clunits.DiphoneUnitDatabase" fork="true">
