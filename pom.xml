                </goals>
                <configuration>
                  <target>
                    <property name="project.resources.directory" value="src/main/voices" />
                    <property name="project.testResources.directory" value="${project.build.outputDirectory}" />
                    <java classname="com.sun.speech.freetts.lexicon.LetterToSoundImpl" fork="true">
                      <classpath>
                        <path refid="maven.compile.classpath"/>
                        <path refid="maven.test.classpath"/>
                      </classpath>
                      <arg line="-src ${project.resources.directory}/com/sun/speech/freetts/en/us" />
                      <arg line="-dest ${project.testResources.directory}/com/sun/speech/freetts/en/us" />
                      <arg line="-name cmudict04_lts" />
                      <arg line="-generate_binary" />
                    </java>
                  </target>
                </configuration>
              </execution>
              <execution>
                <id>cmutimelex_db</id>
                <phase>compile</phase>
                <goals>
                  <goal>run</goal>
                </goals>
                <configuration>
                  <target>
                    <property name="project.resources.directory" value="src/main/voices" />
                    <property name="project.testResources.directory" value="${project.build.outputDirectory}" />
                    <java classname="com.sun.speech.freetts.en.us.CMULexicon" fork="true">
                      <classpath>
                        <path refid="maven.compile.classpath"/>
                        <path refid="maven.test.classpath"/>
                      </classpath>
                      <arg line="-src ${project.resources.directory}/com/sun/speech/freetts/en/us" />
                      <arg line="-dest ${project.testResources.directory}/com/sun/speech/freetts/en/us" />
                      <arg line="-name cmutimelex" />
                      <arg line="-generate_binary" />
                    </java>
                  </target>
                </configuration>
              </execution>
              <execution>
                <id>cmutimelex_lts_db</id>
                <phase>compile</phase>
